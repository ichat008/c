/* 
  Name:			Rotate In Up Left
  Type:			Entrance
  TypeSecond:   Sport
  Description:	Rotate in Effect
  Version:		2.0	
  locale:		{"zh-Hans":"左上旋转进入", "en-US":"Rotate In Up Left"}
 */
 
@keyframes rotateInUpLeft {
  0% {
    transform-origin: left bottom;
    transform: rotate3d(0, 0, 1, 45deg);
    opacity: 0;
  }

  100% {
    transform-origin: left bottom;
    transform: none;
    opacity: 1;
  }
}

.rotateInUpLeft {
  animation-name: rotateInUpLeft;
}